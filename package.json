  },
  "workspaces": [
    "packages/*",
    "build-settings",
    "wb-codemod"
  ],
  "resolutions": {
    "@figspec/components": "^2.0.4",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "18",
    "@types/react-dom": "18",
    "strip-ansi": "6.0.1",
    "strip-ansi-explanation": "There's an issue with strip-ansi v7 which causes conflicts with the Khan/changeset-per-package action"
  }
}
