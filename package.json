{
  "private": true,
  "name": "wonder-blocks",
  "version": "0.0.1",
  "description": "React components for Wonder Blocks design system.",
  "main": "index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "alex": "alex packages/",
    "build": "rollup -c ./build-settings/rollup.config.js && node ./utils/package-flow-types.js",
    "build-storybook": "build-storybook",
    "clean": "rm -rf packages/wonder-blocks-*/dist && rm -rf packages/wonder-blocks-*/node_modules",
    "coverage": "yarn run test:coverage && open coverage/lcov-report/index.html",
    "coveragefor": "bash -c 'yarn --silent jest --coverage --collectCoverageFrom=**/$0 $(node ./utils/test-file-for.js \"$0\")'",
    "coverage:ci": "yarn run jest --coverage",
    "flow-ci": "flow check --max-workers 0",
    "flow": "flow",
    "jest": "jest --config config/jest/test.config.js",
    "lint:watch": "esw --watch --config ./eslint/eslintrc packages",
    "lint": "eslint --config .eslintrc.js packages",
    "lint:ci": "eslint --config .eslintrc.js",
    "publish:ci": "node utils/publish/pre-publish-check-ci.js && git diff --stat --exit-code HEAD && yarn build && changeset publish",
    "start": "start-storybook -p 6061",
    "test:common": "yarn run build && yarn run lint && yarn run alex && yarn run flow",
    "test:coverage": "yarn run test:common && yarn run jest --coverage",
    "test": "yarn run test:common && yarn run jest"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/plugin-transform-flow-strip-types": "^7.19.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/plugin-transform-spread": "7.12.1",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.18.6",
    "@changesets/cli": "^2.18.0",
    "@khanacademy/eslint-config": "^0.2.0",
    "@khanacademy/wonder-stuff-testing": "^2.3.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@storybook/addon-a11y": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/jest": "^0.0.10",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^13.5.0",
    "alex": "^10.0.0",
    "babel-jest": "29.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-transform-flow-enums": "^0.0.2",
    "chromatic": "^6.0.4",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ft-flow": "^2.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "27.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-monorepo": "^0.3.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-testing-library": "^5.0.0",
    "eslint-watch": "^8.0.0",
    "flow-bin": "0.188.0",
    "flow-enums-runtime": "^0.0.6",
    "glob": "^8.0.3",
    "jest": "^29.0.1",
    "jest-date-mock": "^1.0.8",
    "jest-environment-jsdom": "^28.1.2",
    "jest-extended": "^2.0.0",
    "prettier": "^2.8.1",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.74.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.0.0-beta.2",
    "storybook-addon-designs": "^6.3.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.6",
    "@khanacademy/wonder-stuff-core": "^1.2.0",
    "@popperjs/core": "^2.11.5",
    "aphrodite": "^1.2.5",
    "moment": "2.24.0",
    "node-fetch": "^2.6.7",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-popper": "^2.2.5",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "react-window": "^1.8.7"
  },
  "workspaces": [
    "packages/*",
    "build-settings"
  ]
}